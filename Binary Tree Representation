class Solution {
    public static void createTree(Node root, ArrayList<Integer> arr) {
        root.left = new Node(arr.get(1));
        root.right = new Node(arr.get(2));
        root.left.left = new Node(arr.get(3));
        root.left.right = new Node(arr.get(4));
        root.right.left = new Node(arr.get(5));
        root.right.right = new Node(arr.get(6));
    }
}    
